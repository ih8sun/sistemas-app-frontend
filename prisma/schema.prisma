generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sensordata {
  id          Int       @id @default(autoincrement())
  fecha       DateTime? @db.Timestamp(6)
  usaqi       Int?
  co2_ppm     Float?
  co2_ppb     Float?
  humedad     Float?
  pm25_ug_m3  Float?
  pm10_ug_m3  Float?
  pm1_ug_m3   Float?
  pm100_ug_m3 Float?
  temperatura Float?
}
